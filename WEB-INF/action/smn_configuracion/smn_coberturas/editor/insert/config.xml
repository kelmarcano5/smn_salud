<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		Insertar registro 
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTableManager</classname>
		<validator>true</validator>
		<transaction>true</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="nextval.sql" source="sql" scope="transaction" />
		<recordset id="query-smn_localidad.sql" source="request" scope="transaction" />
		<recordset id="query-smn_horario.sql" source="request" scope="transaction" />
		<recordset id="query-smn_unidades_servicio.sql" source="request" scope="transaction" />

	</transaction>
	
	
	<query>insert.sql</query>
	<query params="query-smn_localidad.sql">insert-query-smn_rel_cobertura_localidad.sql</query>
	<query params="query-smn_horario.sql">insert-query-smn_rel_cobertura_horario.sql</query>
	<query params="query-smn_unidades_servicio.sql">insert-query-smn_rel_coberturas_unidades_servicios.sql</query>
	

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>script.js</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/javascript</content-type>
		<expiration>0</expiration>
		<print mode="form" recordset="_request" />
		<print mode="form" recordset="nextval.sql" />
	</output>
	
	<!-- Envio de email declarativo -->
	<mail enabled="false">
		<subject>Subject</subject>
		<mail-to>martin.cordova@gmail.com</mail-to>
		<body>body.txt</body>
	</mail>

</config>
