<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='cob_codigo' type='varchar' required='true' label='${lbl:b_cob_code}' maxlen='16'/>
	<parameter id='cob_descripcion' type='varchar' required='true' label='${lbl:b_cob_des}' maxlen='56'/>
	<parameter id='cob_tiempo_servicio' type='integer' required='false' label='${lbl:b_service_time}' maxlen='1'/>
	<parameter id='cob_unidad_tiempo_rf' type='integer' required='false' label='${lbl:b_cob_unidad_tiempo}' maxlen='10'/>
	<parameter id='cob_deducible' type='double' required='false' label='${lbl:b_deducible}' maxlen='10'/>
	<parameter id='cob_otros_centros' type='varchar' required='true' label='${lbl:b_another_centers}' maxlen='1'/>
	<parameter id='cob_cantidad_servicio' type='integer' required='false' label='${lbl:b_qty}' maxlen='10'/>
	<parameter id='cob_cobertura_desde' type='double' required='false' label='${lbl:b_coverage_from}' maxlen='10'/>
	<parameter id='cob_cobertura_hasta' type='double' required='false' label='${lbl:b_coverage_until}' maxlen='10'/>
	<parameter id='cob_porc_deducible' type='double' required='false' label='${lbl:b_porcentaje_deducible}' maxlen='10'/>
	<parameter id='id' type='integer' required='true' label='${lbl:b_cobertura}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.ArrayNotEmptyValidator' on-error-label='Seleccione al menos un ítem.' parameter-name='smn_localidad_id' id='smn_localidad_id'/>
	<custom-validator classname='dinamica.validators.ArrayToRecordset' paramName='smn_localidad_id' recordsetId='query-smn_localidad.sql' colName='smn_localidad_id' colType='integer'/>
	<custom-validator classname='dinamica.validators.ArrayNotEmptyValidator' on-error-label='Seleccione al menos un ítem.' parameter-name='smn_horarios_id' id='smn_horarios_id'/>
	<custom-validator classname='dinamica.validators.ArrayToRecordset' paramName='smn_horarios_id' recordsetId='query-smn_horario.sql' colName='smn_horarios_id' colType='integer'/>

	<parameter id='smn_unidades_servicios_id' type='varchar' required='true' label='Ítems'/>
	<custom-validator classname='dinamica.validators.StringToRecordset' paramName='smn_unidades_servicios_id' recordsetId='query-smn_unidades_servicio.sql' separatorChar=';' colName='smn_unidades_servicios_id' colType='integer'/>
	<parameter id='smn_tipo_coberturas_id' type='integer' required='true' label='${lbl:b_tipo_coberturas}' maxlen='10'/>
	
</validator>

