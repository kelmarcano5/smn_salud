<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='pat_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='pat_codigo' rule='like' />
	<parameter id='pat_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='pat_descripcion' rule='like' />
	<parameter id='pat_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
	
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='pat_estatus' rule='like' />
	<parameter id='pat_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='pat_fecha_registro_ini' date2='pat_fecha_registro_fin' id='pat_fecha_registro_ini' />
	<parameter id='pat_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
