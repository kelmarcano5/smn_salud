<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_rel_services_pathology}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_patologia_id.sql' source='sql' scope='transaction'/><recordset id='query-smn_servicio_rf.sql' source='sql' scope='transaction'/>
			<recordset id="servicio_id" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_patologia_id.sql' tag='smn_patologia_id_rows'/><print mode='table' recordset='query-smn_servicio_rf.sql' tag='smn_servicio_rf_rows'/>
		<print mode="form" recordset="servicio_id"/>
	</output>

</config>
