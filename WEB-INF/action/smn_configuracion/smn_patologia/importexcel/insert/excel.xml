<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='pat_codigo' type='varchar' required='true' maxlen='48'/>
	<parameter id='pat_descripcion' type='varchar' required='true' maxlen='100'/>
	<parameter id='pat_estatus' type='varchar' required='true' maxlen='2'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='pat_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='pat_descripcion'/>


</validator>
