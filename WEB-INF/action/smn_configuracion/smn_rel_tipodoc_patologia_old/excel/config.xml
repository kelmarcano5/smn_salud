<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnreltipodocpatologia.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnreltipodocpatologia.sql' sheetname='${lbl:b_relacion_tipodoc_patologia}' filename='smn_rel_tipodoc_patologia.xls'>
		<col id='smn_tipo_documento_id_combo' label='${lbl:b_document_type}' />
		<col id='smn_patologia_id_combo' label='${lbl:b_pathology}' />
	</excel>

</config>