<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='smn_tramos_id' type='integer' required='true' label='${lbl:b_cod_tramo}' maxlen='10'/>
	<parameter id='trd_hora_descanso_desde' type='varchar' required='true' label='${lbl:b_hor_inic_desc}' maxlen='5' regexp='^([0-9]|0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]$' regexp-error-label='Formato de hora incorrecto'/>
	<parameter id='trd_hora_descanso_hasta' type='varchar' required='true' label='${lbl:b_hor_fin_desc}' maxlen='5' regexp='^([0-9]|0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]$' regexp-error-label='Formato de hora incorrecto'/>
	<parameter id='id' type='integer' required='true' label='${lbl:b_tramos_descanso}' maxlen='10'/>


</validator>
