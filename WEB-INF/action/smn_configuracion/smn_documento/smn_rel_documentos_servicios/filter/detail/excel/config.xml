<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="2" cellStep="0" name="${lbl:b_relacion_documento_servicio}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_relacion_documento_servicio}' 
		filename='${lbl:b_relacion_documento_servicio}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_documentos_id' label='${lbl:b_contrato_doc}' />
		<col id='smn_servicios_id' label='${lbl:b_service}' />
		<col id='rdt_tipo_generacion' label='${lbl:b_tipo_generacion}' />
		<col id='rdt_hora_generacion' label='${lbl:b_hora_generacion}' />

	</excel>

</config>
