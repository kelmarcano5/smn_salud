<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_document}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_tipo_documento_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_documento_general_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_categoria_justificacion_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_servicios_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_entidades_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_sucursales_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_caja_rf.sql' source='sql' scope='transaction'/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_tipo_documento_id.sql' tag='smn_tipo_documento_id_rows'/>
		<print mode='table' recordset='query-smn_documento_general_rf.sql' tag='smn_documento_general_rf_rows'/>
		<print mode='table' recordset='query-smn_categoria_justificacion_rf.sql' tag='smn_categoria_justificacion_rf_rows'/>
		<print mode='table' recordset='query-smn_servicios_rf.sql' tag='smn_servicios_rf_rows'/>
        <print mode='table' recordset='query-smn_entidades_rf.sql' tag='smn_entidades_rf_rows'/>
        <print mode='table' recordset='query-smn_sucursales_rf.sql' tag='smn_sucursales_rf_rows'/>
         <print mode='table' recordset='query-smn_caja_rf.sql' tag='smn_caja_rf_rows'/>
		<print mode="form" recordset="_request" /> 
	</output>

</config>

