<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_activo_fijo_rf' type='integer' required='true' maxlen='10'/>
	<parameter id='hab_codigo' type='varchar' required='true' maxlen='64'/>
	<parameter id='hab_descripcion' type='varchar' required='true' maxlen='100'/>
	<parameter id='hab_estatus' type='varchar' required='true' maxlen='2'/>
	<parameter id='hab_clase' type='varchar' required='true' maxlen='1'/>
	<parameter id='hab_tipo_servicio_desc_ref' type='varchar' required='true' maxlen='2'/>

	<parameter id='hab_tipo_servicio_ref' type='integer' required='false' maxlen='2'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='hab_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='hab_descripcion'/>


	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: hab_tipo_servicio' 
		parameter='hab_tipo_servicio_ref' 
		sql='hab_tipo_servicio.sql' 
		id='hab_tipo_servicio_ref'/>

</validator>
