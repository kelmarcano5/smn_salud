<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_activo_fijo_rf' type='integer' required='false' label='${lbl:b_activo_fijo}' maxlen='10'/>
	<parameter id='hab_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='64'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='hab_codigo' rule='like' />
	<parameter id='hab_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='hab_descripcion' rule='like' />
	<parameter id='hab_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='hab_estatus' rule='like' />
	<parameter id='hab_clase' type='varchar' required='false' label='${lbl:b_class}' maxlen='1'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='hab_clase' rule='like' />
	<parameter id='hab_tipo_servicio' type='integer' required='false' label='${lbl:b_types_services}' maxlen='2'/>
	<parameter id='hab_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='hab_fecha_registro_ini' date2='hab_fecha_registro_fin' id='hab_fecha_registro_ini' />
	<parameter id='hab_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
