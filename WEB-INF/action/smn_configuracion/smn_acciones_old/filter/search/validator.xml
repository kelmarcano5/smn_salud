<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='acc_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='64'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='acc_codigo' rule='like' />
	<parameter id='acc_descripcion' type='varchar' required='false' label='${lbl:b_desecription}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='acc_descripcion' rule='like' />
	<parameter id='acc_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='acc_fecha_registro_ini' date2='acc_fecha_registro_fin' id='acc_fecha_registro_ini' />
	<parameter id='acc_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
