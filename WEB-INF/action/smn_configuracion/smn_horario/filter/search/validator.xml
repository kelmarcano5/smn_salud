<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='hor_codigo' type='varchar' required='false' label='${lbl:b_hor_cod}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='hor_codigo' rule='like' />
	<parameter id='hor_descripcion' type='varchar' required='false' label='${lbl:b_hor_desc}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='hor_descripcion' rule='like' />
	<parameter id='hor_fecha_registro' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
