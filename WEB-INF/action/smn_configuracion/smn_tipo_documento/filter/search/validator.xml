<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='tdc_codigo' type='varchar' required='false' label='${lbl:b_tdc_codigo}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tdc_codigo' rule='like' />
	<parameter id='tdc_nombre' type='varchar' required='false' label='${lbl:b_tdc_nombre}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tdc_nombre' rule='like' />
	<parameter id='tdc_fecha_registro' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
