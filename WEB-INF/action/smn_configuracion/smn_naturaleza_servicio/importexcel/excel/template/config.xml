<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnnaturalezaservicio.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_service_nature}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_service_nature}">
				<col id='smn_tipo_servicio_rf' label='${lbl:b_types_services}' />
		<col id='nts_naturaleza' label='${lbl:b_nature}' />
		<col id='nts_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
