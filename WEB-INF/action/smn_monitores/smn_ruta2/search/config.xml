<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		Ejecutar consulta y enrutar dependiendo de que un recordset
		tenga o no registros
	</summary>

	<log>false</log>

	<!--configura el control de paginacion-->
	<!-- <set-request-attribute id="paging.recordset" value="query-smnrutapaciente.sql" /> -->
	<set-request-attribute id="paging.recordset" value="query-base.sql" />
	<set-request-attribute id="paging.pagesize" value="5" />

	<transaction>
		<classname>dinamica.GenericSearch</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
	</transaction>
	<recordset id="query-base.sql" source="sql" scope="transaction" />
	<recordset id="query-total.sql" source="sql" scope="transaction" />

	<searchCols id="query-base.sql">smn_sucursal_rf;smn_entidad_rf;rta_ticket;rta_numero_documento_identidad;smn_auxiliar_rf;smn_prestador_servicio_rf;smn_grupo_prestador_rf;tipo_unidad_rp;smn_unidad_servicio_rf;smn_area_servicio_rf;rta_estatus;rta_ejecutivo_asignado;rta_hora_llegada_unidad;rta_hora_salida_unidad;igs_fecha_ingreso_desde;igs_fecha_ingreso_hasta</searchCols>

	<searchCols id="query-total.sql">smn_sucursal_rf;smn_entidad_rf;rta_ticket;rta_numero_documento_identidad;smn_auxiliar_rf;smn_prestador_servicio_rf;smn_grupo_prestador_rf;tipo_unidad_rp;smn_unidad_servicio_rf;smn_area_servicio_rf;rta_estatus;rta_ejecutivo_asignado;rta_hora_llegada_unidad;rta_hora_salida_unidad;igs_fecha_ingreso_desde;igs_fecha_ingreso_hasta</searchCols>

	<print mode="table" recordset="query-base.sql" tag="rows" alternate-colors="true" />
	<print mode="table" recordset="query-total.sql" tag="rows2" alternate-colors="true" />
	

	<on-exit return-code="0" forward-to="/action/initview" />
	<on-exit return-code="1" forward-to="${def:actionroot}/notfound" />

</config>
