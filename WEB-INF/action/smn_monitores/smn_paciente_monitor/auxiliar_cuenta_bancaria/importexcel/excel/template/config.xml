<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarcuentabancaria.sql" source="session" scope="transaction" />
		<recordset id='query-smnentidadesfinancieras.sql' source='sql' scope='transaction' />
	<recordset id='query-smntipocuentabanco.sql' source='sql' scope='transaction' />
	
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_rel_auxiliary_bank_account_person_nature}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_rel_auxiliary_bank_account_person_nature}">
				<col id='smn_clase_auxiliar_rf' label='${lbl:b_class_auxiliary}' />
		<col id='smn_auxiliar_rf' label='${lbl:b_auxiliary}' />
		<col id='acb_num_cuenta' label='${lbl:b_account_number}' />
		<col id='acb_tipo_cta_bancob_auxiliary' label='${lbl:b_account_bank_type}' />
		<col id='acb_banco_perteneceb_auxiliary' label='${lbl:b_bank}' />
		<col id='acb_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
		<sheet recordset='query-smnentidadesfinancieras.sql' sheetname=''>
		<col id='efi_nombre' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smntipocuentabanco.sql' sheetname=''>
		<col id='tcb_descripcion' label='${lbl:b_description}' />
	</sheet>


	</excel>

</config>
