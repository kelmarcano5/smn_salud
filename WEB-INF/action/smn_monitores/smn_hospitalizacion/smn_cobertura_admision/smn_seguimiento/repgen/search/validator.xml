<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_ingreso_id' type='integer' required='false' label='${lbl:b_ingreso}' maxlen='10'/>
	<parameter id='seg_fecha' type='date' required='false' label='${lbl:b_date}' maxlen='10'/>
	<parameter id='seg_hora_seg' type='varchar' required='false' label='${lbl:b_hour}' maxlen='8'/>
	<parameter id='seg_accion' type='integer' required='false' label='${lbl:b_action}' maxlen='10'/>
	<parameter id='seg_observacion' type='integer' required='false' label='${lbl:b_observation}' maxlen='10'/>
	<parameter id='seg_estatus' type='integer' required='false' label='${lbl:b_estatus}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='seg_hora_seg'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
