			<colname>aux_unidad_med_camisa_rf</colname>
			<colname>aux_talla_pantalon</colname>
			<colname>aux_unidad_med_pantalon_rf</colname>
			<colname>aux_talla_zapato</colname>
			<colname>aux_unidad_med_zapato_rf</colname>
