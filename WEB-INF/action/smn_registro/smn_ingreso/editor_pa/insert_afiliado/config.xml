			<colname>aux_observacion</colname>
			<colname>aux_peso</colname>
			<colname>aux_unidad_med_peso_rf</colname>
			<colname>aux_estatura</colname>
			<colname>aux_unidad_med_estatura_rf</colname>
			<colname>aux_lateralidad</colname>
