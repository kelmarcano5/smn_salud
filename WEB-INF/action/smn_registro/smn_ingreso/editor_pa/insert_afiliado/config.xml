			<!--<colname>smn_tipo_contactos_rf</colname>
			<colname>auc_descripcion</colname>

			<colname>smn_tipo_contactos_rf1</colname>
			<colname>auc_descripcion1</colname>

			<colname>apc_apellidos</colname>
			<colname>apc_nombres</colname>
			<colname>apc_telefono_contacto</colname>
			<colname>apc_email</colname>
			<colname>apc_direccion</colname>-->
		
		</cols>
		<table>smn_base.smn_auxiliar</table>
		<oper>AÑADIR</oper>
		<area>Carga de datos</area>
		<!--
			query para obtener el ID generado o nombre de 
			la columna que contiene el pkey, si es query, 
			el alias del campo debe ser "id"
		-->
		<pkey>select ${seq:currval@smn_base.seq_smn_auxiliar} as id</pkey>
	
	</audit>

	<transaction>
		<classname>dinamica.audit.AuditableTransaction</classname>
		<validator>true</validator>
		<transaction>true</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="nextval.sql" source="sql" scope="transaction" />
		<!--<recordset id="nextval1.sql" source="sql" scope="transaction" />-->
		<recordset id="nextval2.sql" source="sql" scope="transaction" />
		<recordset id="nextval4.sql" source="sql" scope="transaction" />
		<recordset id="nextval3numhist.sql" source="sql" scope="transaction" />
	</transaction>

	<query>insert.sql</query>
	<query>insert-contact.sql</query>
	<!-- <query>insert-contact1.sql</query>
	<query>insert-person_contact.sql</query> -->
	<!--<query params="query-tipo_doc.sql">insert.sql</query>-->

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>script.js</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/javascript</content-type>
		<expiration>0</expiration>
		<print mode="form" recordset="_request" />
		<print mode="form" recordset="nextval.sql" />
		<!--<print mode="form" recordset="nextval1.sql" />-->
		<print mode="form" recordset="nextval2.sql" />
		<print mode="form" recordset="nextval2.sql" />
		<print mode="form" recordset="nextval4.sql" />
		<print mode="form" recordset="nextval3numhist.sql" />
	</output>
	
	<!-- Envio de email declarativo -->
	<mail enabled="false">
		<subject>Subject</subject>
		<mail-to>martin.cordova@gmail.com</mail-to>
		<body>body.txt</body>
	</mail>

</config>
