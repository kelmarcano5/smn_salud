			<colname>aux_tipo_doc_oficial_rf</colname>
			<colname>aux_num_doc_oficial</colname>
			<colname>aux_rif</colname>
			<colname>aux_sexo</colname>
			<colname>aux_estado_civil</colname>
			<colname>aux_fecha_nac</colname>
