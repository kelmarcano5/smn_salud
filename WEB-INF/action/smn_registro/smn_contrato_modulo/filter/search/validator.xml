<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_contrato_base_rf' type='integer' required='false' label='${lbl:b_contrato_base}' maxlen='10'/>
	<parameter id='smn_documento_id' type='integer' required='false' label='${lbl:b_contrato_doc}' maxlen='10'/>
	<parameter id='ctm_numero_contrato' type='integer' required='false' label='${lbl:b_number_contract}' maxlen='10'/>
	<parameter id='smn_contratante_id' type='integer' required='false' label='${lbl:b_contratante}' maxlen='10'/>
	<parameter id='smn_planes_id' type='integer' required='false' label='${lbl:b_planes}' maxlen='10'/>
	<parameter id='ctm_estatus_proceso' type='varchar' required='false' label='${lbl:b_status_process}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='ctm_estatus_proceso' rule='like' />
	<parameter id='ctm_estatus_financiero' type='varchar' required='false' label='${lbl:b_discount}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='ctm_estatus_financiero' rule='like' />
	<parameter id='ctm_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='ctm_fecha_registro_ini' date2='ctm_fecha_registro_fin' id='ctm_fecha_registro_ini' />
	<parameter id='ctm_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
