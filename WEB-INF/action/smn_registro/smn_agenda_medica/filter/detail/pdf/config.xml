<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />

	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_medical_timetable}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_medical_timetable}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='query.sql' title='${lbl:b_medical_timetable}' width='60' spacingBefore='40'>
			<col name='smn_agenda_id' title='${lbl:b_timetable}' align='left'  />
			<col name='smn_entidades_rf' title='${lbl:b_entities}' align='left'  />
			<col name='smn_sucursales_rf' title='${lbl:b_subsidiary}' align='left'  />
			<col name='smn_areas_servicios_rf' title='${lbl:b_service_areas}' align='left'  />
			<col name='smn_unidades_servicios_rf' title='${lbl:b_service_units}' align='left'  />
			<col name='smn_grupo_prestador_servicio_rf' title='${lbl:b_group_of_provider}' align='left'  />
			<col name='smn_prestador_servicio_rf' title='${lbl:b_service_providers}' align='left'  />
			<col name='smn_especialidad_rf' title='${lbl:b_speciality}' align='left'  />
			<col name='age_descripcion' title='${lbl:b_jornada_desde}' align='left'  />
			<col name='agm_frecuencia' title='${lbl:b_frecuency}' align='left'  />
			<col name='agm_cantidad_frecuencia' title='${lbl:b_frecuency_quantity}' align='left'  />
			<col name='agm_estatus' title='${lbl:b_status}' align='left' />
			<col name='agm_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
		</record>


	</pdf-report>
</config>
