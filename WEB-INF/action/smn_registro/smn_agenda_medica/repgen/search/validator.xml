<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_agenda_id' type='integer' required='false' label='${lbl:b_timetable}' maxlen='10'/>
	<parameter id='smn_entidades_rf' type='integer' required='false' label='${lbl:b_entities}' maxlen='10'/>
	<parameter id='smn_sucursales_rf' type='integer' required='false' label='${lbl:b_subsidiary}' maxlen='10'/>
	<parameter id='smn_areas_servicios_rf' type='integer' required='false' label='${lbl:b_service_areas}' maxlen='10'/>
	<parameter id='smn_unidades_servicios_rf' type='integer' required='false' label='${lbl:b_service_units}' maxlen='10'/>
	<parameter id='smn_grupo_prestador_servicio_rf' type='integer' required='false' label='${lbl:b_group_of_provider}' maxlen='10'/>
	<parameter id='smn_prestador_servicio_rf' type='integer' required='false' label='${lbl:b_service_providers}' maxlen='10'/>
	<parameter id='smn_especialidad_rf' type='integer' required='false' label='${lbl:b_speciality}' maxlen='10'/>
	<parameter id='agm_jornada_desde_rf' type='integer' required='false' label='${lbl:b_jornada_desde}' maxlen='10'/>
	<parameter id='agm_frecuencia' type='integer' required='false' label='${lbl:b_frecuency}' maxlen='10'/>
	<parameter id='agm_cantidad_frecuencia' type='integer' required='false' label='${lbl:b_frecuency_quantity}' maxlen='10'/>
	<parameter id='agm_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='agm_estatus'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
