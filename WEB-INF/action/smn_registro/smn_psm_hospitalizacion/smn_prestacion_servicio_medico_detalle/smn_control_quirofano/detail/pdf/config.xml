<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />

	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_control_quirofano}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_control_quirofano}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='query.sql' title='${lbl:b_control_quirofano}' width='60' spacingBefore='40'>
			<col name='smn_agenda_rf' title='${lbl:b_schedule}' align='left' format='######' />
			<col name='smn_presupuesto_id' title='${lbl:b_presupuesto}' align='left' format='######' />
			<col name='smn_ingreso_id' title='${lbl:b_ingreso}' align='left' format='######' />
			<col name='smn_prestacion_servicio_cabecera_id' title='${lbl:b_psm_cabecera}' align='left' format='######' />
			<col name='smn_prestación_servicio_medico_detalle_id' title='${lbl:b_psm_detalle}' align='left' format='######' />
			<col name='smn_servicios_rf' title='${lbl:b_services}' align='left' />
			<col name='ccq_hora_entrada' title='${lbl:b_entry_hour}' align='left' />
			<col name='ccq_hora_salida' title='${lbl:b_exit_hour}' align='left' format='######' />
			<col name='ccq_fecha_quirofano' title='${lbl:b_fecha_quirofano}' align='left' format='dd-MM-yyyy' />
			<col name='smn_habitaciones_id' title='${lbl:b_habitaciones}' align='left' format='######' />
			<col name='ccq_cantidad_horas' title='${lbl:b_entry_hour}' align='left' format='######' />
			<col name='smn_unidad_medida_rf' title='${lbl:b_measurement_unit}' align='left' format='######' />
			<col name='ccq_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
		</record>


	</pdf-report>
</config>
