<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="2" cellStep="0" name="${lbl:b_relacion_quirofano_prest}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_relacion_quirofano_prest}' 
		filename='${lbl:b_relacion_quirofano_prest}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_control_quirofano_id' label='${lbl:b_control_quirofano}' />
		<col id='smn_servicios_rf' label='${lbl:b_services}' />
		<col id='smn_componente_rf' label='${lbl:b_component}' />
		<col id='smn_grupo_prestador_servicio_rf' label='${lbl:b_grupo_prestador_servicio}' />
		<col id='smn_prestador_servicio_rf' label='${lbl:b_service_provider}' />

	</excel>

</config>
