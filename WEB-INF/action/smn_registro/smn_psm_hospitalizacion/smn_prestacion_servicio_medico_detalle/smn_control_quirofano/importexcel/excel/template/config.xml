<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncontrolquirofano.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_control_quirofano}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_control_quirofano}">
				<col id='smn_agenda_rf' label='${lbl:b_schedule}' />
		<col id='smn_presupuesto_id' label='${lbl:b_presupuesto}' />
		<col id='smn_ingreso_id' label='${lbl:b_ingreso}' />
		<col id='smn_prestacion_servicio_cabecera_id' label='${lbl:b_psm_cabecera}' />
		<col id='smn_prestación_servicio_medico_detalle_id' label='${lbl:b_psm_detalle}' />
		<col id='smn_servicios_rf' label='${lbl:b_services}' />
		<col id='ccq_hora_entrada' label='${lbl:b_entry_hour}' />
		<col id='ccq_hora_salida' label='${lbl:b_exit_hour}' />
		<col id='ccq_fecha_quirofano' label='${lbl:b_fecha_quirofano}' />
		<col id='smn_habitaciones_id' label='${lbl:b_habitaciones}' />
		<col id='ccq_cantidad_horas' label='${lbl:b_entry_hour}' />
		<col id='smn_unidad_medida_rf' label='${lbl:b_measurement_unit}' />
		<col id='ccq_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
