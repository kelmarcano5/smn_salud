<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_ingreso_id' type='integer' required='false' label='${lbl:b_ingreso}' maxlen='10'/>
	<parameter id='omd_numero_orden' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='omd_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='256'/>
	<parameter id='smn_prestador_servicio_rf' type='integer' required='false' label='${lbl:b_service_providers}' maxlen='10'/>
	<parameter id='omd_clase_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary_class}' maxlen='10'/>
	<parameter id='omd_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='omd_num_doc_oficial' type='varchar' required='false' label='${lbl:b_budget_number}' maxlen='12'/>
	<parameter id='omd_tipo_orden' type='varchar' required='false' label='${lbl:b_order_type}' maxlen='2'/>
	<parameter id='omd_indicaciones' type='varchar' required='false' label='${lbl:b_indications}' maxlen='500'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='omd_numero_orden'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
parameter='omd_descripcion'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='omd_num_doc_oficial'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='omd_tipo_orden'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='omd_indicaciones'
		rule='contains' />


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='omd_descripcion'/>


	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
