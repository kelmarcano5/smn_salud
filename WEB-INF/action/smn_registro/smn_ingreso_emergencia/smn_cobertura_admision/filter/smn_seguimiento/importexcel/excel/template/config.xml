<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnseguimiento.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_seguimiento}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_seguimiento}">
				<col id='smn_ingreso_id' label='${lbl:b_ingreso}' />
		<col id='seg_fecha' label='${lbl:b_date}' />
		<col id='seg_hora_seg' label='${lbl:b_hour}' />
		<col id='seg_accion' label='${lbl:b_action}' />
		<col id='seg_observacion' label='${lbl:b_observation}' />
		<col id='seg_estatus' label='${lbl:b_estatus}' />
		<col id='seg_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
