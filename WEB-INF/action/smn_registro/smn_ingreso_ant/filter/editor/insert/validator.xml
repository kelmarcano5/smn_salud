<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='smn_doc_orig' type='integer' required='false' label='${lbl:b_doc_orig}' maxlen='10'/>
	<parameter id='smn_documento_id' type='integer' required='true' label='${lbl:b_document}' maxlen='10'/>
	<parameter id='igs_doc_origen' type='integer' required='false' label='${lbl:b_num_doc_orig}' maxlen='10'/>
	<parameter id='smn_entidades_rf' type='integer' required='false' label='${lbl:b_entities}' maxlen='10'/>
	<parameter id='smn_sucursales_rf' type='integer' required='false' label='${lbl:b_subsidiaries}' maxlen='10'/>
	<parameter id='smn_areas_servicios_rf' type='integer' required='true' label='${lbl:b_service_area}' maxlen='10'/>
	<parameter id='smn_unidades_servicios_rf' type='integer' required='true' label='${lbl:b_service_units}' maxlen='10'/>
