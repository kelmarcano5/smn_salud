<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_budget_request_honorary}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<record recordset='query.sql' title='${lbl:b_budget_request_honorary}' width='60' spacingBefore='40'>
			<col name='smn_solicitud_presupuesto_id' title='${lbl:b_budget_request_honorary}' align='left' format='######' />
			<col name='smn_servicios_rf_combo' title='${lbl:b_services}' align='left' format='######' />
			<col name='smn_componentes_rf_combo' title='${lbl:b_component}' align='left' format='######' />
			<col name='smn_grupo_prestador_servicio_rf_combo' title='${lbl:b_grupo_prestador_servicio}' align='left' format='######' />
			<col name='smn_prestador_serviciorf_combo' title='${lbl:b_service_provider}' align='left' format='######' />
			<col name='rsh_monto_solicitado_ml' title='${lbl:b_amount_ml}' align='left' format='#,###,##0.00' />
			<col name='rsh_monto_solicitado_ma' title='${lbl:b_amount_ma}' align='left' format='#,###,##0.00' />
			<col name='smn_tasa_id_combo' title='${lbl:b_tasa}' align='left' format='######' />
			<col name='smn_moneda_id_combo' title='${lbl:b_coins}' align='left' format='######' />
			<col name='rsh_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
		</record>

	</pdf-report>
</config>