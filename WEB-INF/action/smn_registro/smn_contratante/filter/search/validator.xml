<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='cnt_numero_control' type='integer' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='smn_documento_id' type='integer' required='false' label='${lbl:b_code}' maxlen='16'/>	
	<parameter id='cnt_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' 
	on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que 
	[${lbl:b_store_date_less_than} menor o igual que].' date1='cnt_fecha_registro_ini' 
	date2='cnt_fecha_registro_fin' id='cnt_fecha_registro_ini' />
	
	<parameter id='cnt_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>