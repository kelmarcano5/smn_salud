<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_contratante}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_documento_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_clase_auxiliar_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_auxiliar_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_plan_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_estructura_organizacional_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_baremos_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_clase_baremos_rf.sql' source='sql' scope='transaction'/>
				<recordset id='query-smn_documento_ingreso_id.sql' source='sql' scope='transaction'/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_documento_id.sql' tag='smn_documento_id_rows'/>
		<print mode='table' recordset='query-smn_clase_auxiliar_rf.sql' tag='smn_clase_auxiliar_rf_rows'/>
		<print mode='table' recordset='query-smn_auxiliar_rf.sql' tag='smn_auxiliar_rf_rows'/>
		<print mode='table' recordset='query-smn_plan_id.sql' tag='smn_plan_id_rows'/>
		<print mode='table' recordset='query-smn_estructura_organizacional_rf.sql' tag='smn_estructura_organizacional_rf_rows'/>
		<print mode='table' recordset='query-smn_baremos_id.sql' tag='smn_baremos_id_rows'/>
		<print mode='table' recordset='query-smn_clase_baremos_rf.sql' tag='smn_clase_baremos_rf_rows'/>
			<print mode='table' recordset='query-smn_documento_ingreso_id.sql' tag='smn_documento_ingreso_id_rows'/>
		<print mode="form" recordset="_request" /> 

	</output>

</config>
