<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_presupuesto_detalle_id' type='integer' required='false' label='${lbl:b_presupuesto_detalle}' maxlen='10'/>
	<parameter id='smn_presupuesto_id' type='integer' required='false' label='${lbl:b_presupuesto}' maxlen='10'/>
	<parameter id='smn_servicios_rf' type='integer' required='false' label='${lbl:b_services}' maxlen='10'/>
	<parameter id='smn_tipos_componentes_rf' type='varchar' required='false' label='${lbl:b_type_component}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='smn_tipos_componentes_rf' rule='like' />
	<parameter id='smn_componentes_rf' type='integer' required='false' label='${lbl:b_component}' maxlen='10'/>
	<parameter id='prc_visible_rf' type='varchar' required='false' label='${lbl:b_visible}' maxlen='1'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='prc_visible_rf' rule='like' />
	<parameter id='prc_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='prc_fecha_registro_ini' date2='prc_fecha_registro_fin' id='prc_fecha_registro_ini' />
	<parameter id='prc_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
