<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelplanquirurgicoprestador.sql" source="session" scope="transaction" />
		<recordset id="filter.params" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_relacion_plan_quirurgico_prest}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' align="left" />
	<pdf-file-name>${lbl:b_relacion_plan_quirurgico_prest}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='filter.params' title='${lbl:b_filter_find}' noFilterMsg='-- ${lbl:b_all_message} --' width='55' spacingBefore='15'>
			<col name='smn_plan_quirurgico_id' title='${lbl:b_plan_quirurgico}'  format='######' align='left' />
			<col name='smn_servicios_rf' title='${lbl:b_services}'  format='######' align='left' />
			<col name='smn_componentes_rf' title='${lbl:b_component}'  format='######' align='left' />
			<col name='smn_grupo_prestador_servicio_rf' title='${lbl:b_grupo_prestador_servicio}'  format='######' align='left' />
			<col name='smn_prestador_servicio_rf' title='${lbl:b_service_provider}'  format='######' align='left' />

		</record>
		<record recordset='query-smnrelplanquirurgicoprestador.sql' width='100' spacingBefore='10'>
			<col name='smn_plan_quirurgico_id' title='${lbl:b_plan_quirurgico}' width='20' align='left' format='######' />
			<col name='smn_servicios_rf' title='${lbl:b_services}' width='20' align='left' format='######' />
			<col name='smn_componentes_rf' title='${lbl:b_component}' width='20' align='left' format='######' />
			<col name='smn_grupo_prestador_servicio_rf' title='${lbl:b_grupo_prestador_servicio}' width='20' align='left' format='######' />
			<col name='smn_prestador_servicio_rf' title='${lbl:b_service_provider}' width='20' align='left' format='######' />

		</record>

	</pdf-report>
</config>
