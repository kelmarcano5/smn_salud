<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='spr_solicitud' type='integer' required='false' label='${lbl:b_request_number}' maxlen='10'/>
	<parameter id='smn_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='smn_contratante_id' type='integer' required='false' label='${lbl:b_contratante_aux}' maxlen='10'/>
	<parameter id='smn_patologia_id' type='integer' required='false' label='${lbl:b_pathology}' maxlen='10'/>
	<parameter id='spr_estatus' type='varchar' required='false' label='${lbl:b_estatus}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='spr_estatus' rule='like' />
	<parameter id='spr_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='spr_fecha_registro_ini' date2='spr_fecha_registro_fin' id='spr_fecha_registro_ini' />
	<parameter id='spr_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<parameter id="id2" type="integer" required="true" label="ID" maxlen="10" />
</validator>
