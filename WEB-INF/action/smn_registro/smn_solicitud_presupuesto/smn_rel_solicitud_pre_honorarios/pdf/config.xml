<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelsolicitudprehonorarios.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_budget_request_honorary}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_budget_request_honorary}.pdf</pdf-file-name>

	<pdf-report 
		pageSize="letter" 
		rotate="false"
		footer="false" 
		header="true">
		<record recordset='query-smnrelsolicitudprehonorarios.sql' width='100' spacingBefore='40'>
			<col name='smn_solicitud_presupuesto_id' title='${lbl:b_budget_request_honorary}' width='10' align='left' format='######' />
			<col name='smn_servicios_rf' title='${lbl:b_services}' width='10' align='left' format='######' />
			<col name='smn_componentes_rf' title='${lbl:b_component}' width='10' align='left' format='######' />
			<col name='smn_grupo_prestador_servicio_rf' title='${lbl:b_grupo_prestador_servicio}' width='10' align='left' format='######' />
			<col name='smn_prestador_serviciorf' title='${lbl:b_service_provider}' width='10' align='left' format='######' />
			<col name='rsh_monto_solicitado_ml' title='${lbl:b_amount_ml}' width='10' align='left' format='#,###,##0.00' />
			<col name='rsh_monto_solicitado_ma' title='${lbl:b_amount_ma}' width='10' align='left' format='#,###,##0.00' />
			<col name='smn_tasa_id' title='${lbl:b_tasa}' width='10' align='left' format='######' />
			<col name='smn_moneda_id' title='${lbl:b_coins}' width='10' align='left' format='######' />
			<col name='rsh_fecha_registro' title='${lbl:b_store_date}' width='10' align='left' format='dd-MM-yyyy' />

		</record>

	</pdf-report>
</config>
