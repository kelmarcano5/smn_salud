<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_ingreso_cola_virtual}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="user-info.sql" source="sql" scope="session" />
		<recordset id="dispositivo-info.sql" source="sql" scope="session" />
		<recordset id="query-smn_tipo_doc_oficial.sql" source="sql" scope="transaction" />
		<recordset id="query-smn_tipo_doc_oficial.sql" source="sql" scope="transaction" />
		<recordset id="query-smn_contratante_id.sql" source="sql" scope="transaction" />
		<recordset id="query-smn_grupo_prestador_servicio_rf.sql" source="sql" scope="transaction" />
		<recordset id="query-smn_unidad_servicio_rf_pres.sql" source="sql" scope="transaction" />
		<recordset id="ingresocola" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode="form" recordset="user-info.sql" />
		<print mode="form" recordset="dispositivo-info.sql" />
		<print mode='table' recordset='query-smn_tipo_doc_oficial.sql' tag='smn_tipo_doc_oficial_rows'/>
		<print mode='table' recordset='query-smn_contratante_id.sql' tag='smn_contratante_id_rows'/>
		<print mode='table' recordset='query-smn_grupo_prestador_servicio_rf.sql' tag='smn_grupo_prestador_servicio_rf_rows'/>
		<print mode='table' recordset='query-smn_unidad_servicio_rf_pres.sql' tag='smn_unidad_servicio_rf_pres_rows'/>
		<print mode="form" recordset="ingresocola" />

		
	</output>

</config>
