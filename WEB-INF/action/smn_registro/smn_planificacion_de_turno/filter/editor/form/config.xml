<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_plan_turnos}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_usuario_id.sql' source='sql' scope='transaction'/><recordset id='query-smn_rol_id.sql' source='sql' scope='transaction'/><recordset id='query-smn_serie_id.sql' source='sql' scope='transaction'/><recordset id='query-smn_entidades_rf.sql' source='sql' scope='transaction'/><recordset id='query-smn_sucursales_rf.sql' source='sql' scope='transaction'/><recordset id='query-smn_areas_servicios_rf.sql' source='sql' scope='transaction'/><recordset id='query-smn_unidad_servicio_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_puesto_atencion_id.sql' source='sql' scope='transaction'/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_usuario_id.sql' tag='smn_usuario_id_rows'/><print mode='table' recordset='query-smn_rol_id.sql' tag='smn_rol_id_rows'/><print mode='table' recordset='query-smn_serie_id.sql' tag='smn_serie_id_rows'/><print mode='table' recordset='query-smn_entidades_rf.sql' tag='smn_entidades_rf_rows'/><print mode='table' recordset='query-smn_sucursales_rf.sql' tag='smn_sucursales_rf_rows'/><print mode='table' recordset='query-smn_areas_servicios_rf.sql' tag='smn_areas_servicios_rf_rows'/><print mode='table' recordset='query-smn_unidad_servicio_rf.sql' tag='smn_unidad_servicio_rf_rows'/>
		<print mode='table' recordset='query-smn_puesto_atencion_id.sql' tag='smn_puesto_atencion_id_rows'/>
		<print mode="form" recordset="_request" /> 
	</output>

</config>
