<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnplanificaciondeturno.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_plan_turnos}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_plan_turnos}">
				<col id='smn_usuario_id' label='${lbl:b_users}' />
		<col id='smn_rol_id' label='${lbl:b_role}' />
		<col id='ptu_fecha' label='${lbl:b_fecha_atencion}' />
		<col id='smn_serie_id' label='${lbl:b_serie}' />
		<col id='smn_entidades_rf' label='${lbl:b_entities}' />
		<col id='smn_sucursales_rf' label='${lbl:b_subsidiaries}' />
		<col id='smn_areas_servicios_rf' label='${lbl:b_service_area}' />
		<col id='ptu_estatus' label='${lbl:b_status}' />
		<col id='ptu_idioma' label='${lbl:s_profile_lang}' />
		<col id='ptu_usuario' label='${lbl:b_users}' />
		<col id='ptu_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
