<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />

	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_rooms_availability}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_rooms_availability}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='query.sql' title='${lbl:b_rooms_availability}' width='60' spacingBefore='40'>
			<col name='smn_habitaciones_id' title='${lbl:b_room}' align='left' format='######' />
			<col name='smn_ingreso_id' title='${lbl:b_ingreso}' align='left' format='######' />
			<col name='had_fecha_ingreso' title='${lbl:b_date_ing}' align='left' format='dd-MM-yyyy' />
			<col name='had_fecha_egreso' title='${lbl:b_date_egr}' align='left' format='dd-MM-yyyy' />
			<col name='had_estatus' title='${lbl:b_status}' align='left' />
			<col name='had_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
		</record>


	</pdf-report>
</config>
