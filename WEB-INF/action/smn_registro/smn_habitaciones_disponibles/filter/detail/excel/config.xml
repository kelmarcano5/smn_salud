<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_rooms_availability}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_rooms_availability}' 
		filename='${lbl:b_rooms_availability}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_habitaciones_id' label='${lbl:b_activo_fijo}' />
		<col id='smn_ingreso_id' label='${lbl:b_ingreso}' />
		<col id='had_fecha_ingreso' label='${lbl:b_date_ing}' />
		<col id='had_fecha_egreso' label='${lbl:b_date_egr}' />
		<col id='had_estatus' label='${lbl:b_status}' />
		<col id='had_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
