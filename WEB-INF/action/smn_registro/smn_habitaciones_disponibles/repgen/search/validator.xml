<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_habitaciones_id' type='integer' required='false' label='${lbl:b_activo_fijo}' maxlen='10'/>
	<parameter id='smn_ingreso_id' type='integer' required='false' label='${lbl:b_ingreso}' maxlen='10'/>
	<parameter id='had_fecha_ingreso' type='date' required='false' label='${lbl:b_date_ing}' maxlen='10'/>
	<parameter id='had_fecha_egreso' type='date' required='false' label='${lbl:b_date_egr}' maxlen='10'/>
	<parameter id='had_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='had_estatus'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
