<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_serie_id' type='varchar' required='false' label='${lbl:b_serie}' maxlen='2'/>
	<parameter id='smn_auxiliar_rf' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='rta_ticket' type='integer' required='false' label='${lbl:b_rta_ticket}' maxlen='10'/>
	<parameter id='rta_numero_documento_identidad' type='varchar' required='false' label='${lbl:b_rta_documento}' maxlen='20'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='rta_numero_documento_identidad' rule='contains' />
	<!-- <parameter id='rta_fecha_registro' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/> -->
	<parameter id='smn_unidad_servicio_rf' type='integer' required='false' label='${lbl:b_units_service}' maxlen='10'/>
	<parameter id='rta_secuencia' type='integer' required='false' label='${lbl:b_sequence}' maxlen='10'/>	
	<parameter id='rta_estatus' type='varchar' required='false' label='${lbl:b_sequence}' maxlen='10'/>	

	<parameter id='rta_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='rta_fecha_registro_ini' date2='rta_fecha_registro_fin' id='rta_fecha_registro_ini' />
	<parameter id='rta_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
