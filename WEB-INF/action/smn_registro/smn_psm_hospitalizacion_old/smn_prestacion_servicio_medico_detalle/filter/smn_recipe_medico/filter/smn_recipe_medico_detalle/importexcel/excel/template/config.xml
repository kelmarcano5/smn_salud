<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrecipemedicodetalle.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_detail_prescription_medic}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_detail_prescription_medic}">
				<col id='rec_estatus_pl0' label='${lbl:b_prescription_medic}' />
		<col id='smn_principio_activo_rf' label='${lbl:b_active_ingredient}' />
		<col id='smn_item_rf' label='${lbl:b_item}' />
		<col id='smn_presentacion_rf' label='${lbl:b_presentacion}' />
		<col id='smn_unidad_medida_rf' label='${lbl:b_measurement_unit}' />
		<col id='rmd_dosis' label='${lbl:b_dose}' />
		<col id='rmd_frecuencia' label='${lbl:b_frecuency}' />
		<col id='rmd_tiempo_frecuencia' label='${lbl:b_frecuency_time}' />
		<col id='rmd_especificacion' label='${lbl:b_especification}' />
		<col id='rmd_dias_tratamiento' label='${lbl:b_treatment_day}' />
		<col id='rmd_dosis_total' label='${lbl:b_total_dose}' />
		<col id='rmd_observacion' label='${lbl:b_observation}' />
		<col id='rmd_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
