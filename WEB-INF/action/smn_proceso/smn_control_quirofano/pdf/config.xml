<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncontrolquirofano.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_control_quirofano}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_control_quirofano}.pdf</pdf-file-name>

	<pdf-report 
		pageSize="letter" 
		rotate="false"
		footer="false" 
		header="true">
		<record recordset='query-smncontrolquirofano.sql' width='100' spacingBefore='40'>
			<col name='smn_agenda_rf' title='${lbl:b_schedule}' width='10' align='left' format='######' />
			<col name='smn_presupuesto_id' title='${lbl:b_presupuesto}' width='10' align='left' format='######' />
			<col name='smn_ingreso_id' title='${lbl:b_ingreso}' width='10' align='left' format='######' />
			<col name='smn_servicios_rf' title='${lbl:b_services}' width='10' align='left' />
			<col name='ccq_hora_entrada' title='${lbl:b_entry_hour}' width='10' align='left' />
			<col name='ccq_hora_salida' title='${lbl:b_exit_hour}' width='10' align='left' format='######' />
			<col name='ccq_fecha_quirofano' title='${lbl:b_fecha_quirofano}' width='10' align='left' format='dd-MM-yyyy' />
			<col name='smn_habitaciones_id' title='${lbl:b_habitaciones}' width='10' align='left' format='######' />
			<col name='ccq_cantidad_horas' title='${lbl:b_entry_hour}' width='10' align='left' format='######' />
			<col name='ccq_fecha_registro' title='${lbl:b_store_date}' width='10' align='left' format='dd-MM-yyyy' />

		</record>

	</pdf-report>
</config>
