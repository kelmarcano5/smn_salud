<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_agenda_rf' type='integer' required='false' label='${lbl:b_schedule}' maxlen='10'/>
	<parameter id='smn_ingreso_id' type='integer' required='false' label='${lbl:b_ingreso}' maxlen='10'/>
	<parameter id='smn_servicios_rf' type='varchar' required='false' label='${lbl:b_services}' maxlen='8'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='smn_servicios_rf' rule='like' />
	<parameter id='ccq_fecha_quirofano' type='date' required='false' label='${lbl:b_fecha_quirofano}' maxlen='10'/>
	<parameter id='smn_habitaciones_id' type='integer' required='false' label='${lbl:b_habitaciones}' maxlen='10'/>
	<parameter id='ccq_fecha_registro_init' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='ccq_fecha_registro_init' date2='ccq_fecha_registro_fin' id='ccq_fecha_registro_init' />
	<parameter id='ccq_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
