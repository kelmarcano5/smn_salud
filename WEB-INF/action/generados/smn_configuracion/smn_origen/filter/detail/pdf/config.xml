<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_origin}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<record recordset='query.sql' title='${lbl:b_origin}' width='60' spacingBefore='40'>
			<col name='ori_codigo' title='${lbl:b_code}' align='left' />
			<col name='ori_descripcion' title='${lbl:b_description} (CallCenter, Internet, ...)' align='left' />
			<col name='ori_tipo_combo' title='${lbl:b_type}' align='left' />
			<col name='ori_idioma' title='${lbl:s_profile_lang}' align='left' />
			<col name='ori_usuario' title='${lbl:b_users}' align='left' />
			<col name='ori_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
			<col name='ori_hora' title='${lbl:s_loginhist_col2}' align='left' />
		</record>

	</pdf-report>
</config>