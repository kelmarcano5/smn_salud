<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_cobertura}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<record recordset='query.sql' title='${lbl:b_cobertura}' width='60' spacingBefore='40'>
			<col name='cob_codigo' title='${lbl:b_code}' align='left' />
			<col name='cob_descripcion' title='${lbl:b_description}' align='left' />
			<col name='cob_horario_dias' title='${lbl:b_schedule_days}' align='left' />
			<col name='cob_horario_horas' title='${lbl:b_schedule_hours}' align='left' format='######' />
			<col name='cob_area_cobertura_combo' title='${lbl:b_coverage_area}' align='left' format='######' />
			<col name='cob_descuento' title='${lbl:b_discount}' align='left' format='#,###,##0.00' />
			<col name='cob_tiempo_servicio' title='${lbl:b_service_time}' align='left' format='######' />
			<col name='smn_tipo_coberturas_id_combo' title='${lbl:b_tipo_cobertura}' align='left' format='######' />
			<col name='cob_deducible' title='${lbl:b_deducible}' align='left' format='#,###,##0.00' />
			<col name='cob_time_desp_afil' title='${lbl:b_time_after_affiliation}' align='left' format='######' />
			<col name='cob_otros_centros_combo' title='${lbl:b_another_centers}' align='left' />
			<col name='cob_cantidad_servicio' title='${lbl:b_qty}' align='left' format='######' />
			<col name='cob_vigencia_desde' title='${lbl:b_validation_date_from}' align='left' format='dd-MM-yyyy' />
			<col name='cob_vigencia_hasta' title='${lbl:b_validation_date_until}' align='left' format='dd-MM-yyyy' />
			<col name='cob_cobertura_desde' title='${lbl:b_coverage_from}' align='left' format='#,###,##0.00' />
			<col name='cob_cobertura_hasta' title='${lbl:b_coverage_until}' align='left' format='#,###,##0.00' />
			<col name='cob_deducible_centro' title='${lbl:b_deductible_center}' align='left' format='#,###,##0.00' />
			<col name='cob_porc_deducible' title='${lbl:b_porcentaje_deducible}' align='left' format='#,###,##0.00' />
			<col name='cob_idioma' title='${lbl:s_profile_lang}' align='left' />
			<col name='cob_usuario' title='${lbl:b_users}' align='left' />
			<col name='cob_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
			<col name='cob_hora' title='${lbl:s_loginhist_col2}' align='left' />
		</record>

	</pdf-report>
</config>