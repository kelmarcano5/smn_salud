<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='cts_num_control' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='cts_num_doc_oficial' type='varchar' required='false' label='${lbl:b_official_document_number}' maxlen='12'/>
	<parameter id='smn_motivos_id_name' type='varchar' required='false' label='${lbl:b_reason}'/>
	<parameter id='smn_motivos_id' type='integer' required='false' label='${lbl:b_reason}' maxlen='10'/>
	<parameter id='cts_clase_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary_class}' maxlen='10'/>
	<parameter id='cts_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='smn_origen_id_name' type='varchar' required='false' label='${lbl:b_origin}'/>
	<parameter id='smn_origen_id' type='integer' required='false' label='${lbl:b_origin}' maxlen='2'/>
	<parameter id='cts_ordenante' type='integer' required='false' label='${lbl:b_applicant}' maxlen='10'/>
	<parameter id='cts_tipo_orden' type='varchar' required='false' label='${lbl:b_order_type}' maxlen='2'/>
	<parameter id='cts_titular' type='integer' required='false' label='${lbl:${lbl:b_titular}' maxlen='56'/>
	<parameter id='smn_estatus_citas_id_name' type='varchar' required='false' label='${lbl:b_status_appointment}'/>
	<parameter id='smn_estatus_citas_id' type='integer' required='false' label='${lbl:b_status_appointment}' maxlen='10'/>
	<parameter id='smn_relacionados_id_name' type='varchar' required='false' label='${lbl:b_relacionados}'/>
	<parameter id='smn_relacionados_id' type='integer' required='false' label='${lbl:b_relacionados}' maxlen='10'/>
	<parameter id='smn_observaciones_citas_id_name' type='varchar' required='false' label='${lbl:b_appointment_comment}'/>
	<parameter id='smn_observaciones_citas_id' type='integer' required='false' label='${lbl:b_appointment_comment}' maxlen='10'/>
	<parameter id='smn_citas_mov_id_name' type='varchar' required='false' label='${lbl:b_appointment_movement}'/>
	<parameter id='smn_citas_mov_id' type='integer' required='false' label='${lbl:b_appointment_movement}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='cts_num_control'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='cts_num_doc_oficial'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_motivos_id_name'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_origen_id_name'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='cts_tipo_orden'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_estatus_citas_id_name'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_relacionados_id_name'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_observaciones_citas_id_name'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_citas_mov_id_name'
		rule='contains' />




	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
