<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_citas}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_citas}' 
		filename='${lbl:b_citas}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='cts_num_control' label='${lbl:b_code}' />
		<col id='cts_num_doc_oficial' label='${lbl:b_official_document_number}' />
		<col id='cla_nombre_combo' label='${lbl:b_auxiliary_class}' />
		<col id='smn_item_combo' label='${lbl:b_auxiliary}' />
		<col id='prs_apellidos_combo' label='${lbl:b_applicant}' />
		<col id='afi_apellidos_combo' label='${lbl:${lbl:b_titular}' />
		<col id='cts_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
