<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncitasmov.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smncitasmov.sql' sheetname='${lbl:b_appointment_movement}' filename='filter.xls'>
		<col id='ctm_num_control_cita' label='${lbl:b_code}' />
		<col id='ctm_fecha' label='${lbl:b_appointment_date}' />
		<col id='ctm_localidad' label='${lbl:b_location}' />
		<col id='ctm_servicio_combo' label='${lbl:b_service}' />
		<col id='ctm_prestador_servicio_combo' label='${lbl:b_service_providers}' />
		<col id='ctm_estatus_combo' label='${lbl:b_status}' />
		<col id='ctm_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>